{
  "name": "dsnwp/any-content-menu",
  "license": "GPL-2.0-or-later",
  "type": "wordpress-plugin",
  "description": "Display post content as menu item content.",
  "require": {
    "php": ">=7.1"
  },
  "extra": {
  },
  "autoload": {
    "psr-4": {
      "AnyContentMenu\\": "."
    }
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5",
    "wp-coding-standards/wpcs": "^2.3",
    "phpcompatibility/phpcompatibility-wp": "*",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7",
    "consistence/coding-standard": "^3.10",
    "phpstan/phpstan": "^0.12.99",
    "szepeviktor/phpstan-wordpress": "^0.7.7",
    "wpreadme2markdown/wp2md": "*"
  },
  "scripts": {
    "lint": [
      "@phpcs",
      "@phpstan"
    ],
    "phpcs": "./vendor/bin/phpcs -p .",
    "phpstan": "./vendor/bin/phpstan --memory-limit=1G",
    "readme": "./vendor/bin/wp2md -i readme.txt -o README.md"
  }
}
